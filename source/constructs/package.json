{
  "name": "constructs",
  "description": "Serverless Image Handler Constructs",
  "version": "5.2.0",
  "license": "Apache-2.0",
  "bin": {
    "constructs": "bin/constructs.js"
  },
  "scripts": {
    "cdk": "cdk",
    "compile": "echo nothing to do",
    "test:compile": "tsc --noEmit --project tsconfig.jest.json",
    "test": "export BUCKET_NAME=TEST SOLUTION_NAME=serverless-image-handler VERSION=TEST_VERSION && rm -fr dist/ && yarn test:compile && jest --passWithNoTests --all --updateSnapshot && eslint && yarn test:new-image-handler",
    "test:new-image-handler": "cd ../new-image-handler && yarn test",
    "build": "yarn test && yarn compile && yarn synth",
    "build:new-image-handler:docker": "cd ../new-image-handler && yarn build:docker",
    "test:watch": "export BUCKET_NAME=TEST SOLUTION_NAME=serverless-image-handler VERSION=TEST_VERSION && jest --watch",
    "test:update": "export BUCKET_NAME=TEST SOLUTION_NAME=serverless-image-handler VERSION=TEST_VERSION && jest --updateSnapshot",
    "upgrade-dependencies": "export CI=0 && npm-check-updates --upgrade --target=minor --reject='projen' && yarn install --check-files && yarn upgrade",
    "eslint": "eslint --ext .ts,.tsx --fix --no-error-on-unmatched-pattern src test build-tools .projenrc.js",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "diff": "cdk diff",
    "postinstall": "cd ../new-image-handler && yarn"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.110.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "aws-cdk": "1.110.0",
    "eslint": "^7.30.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "jest": "^27.0.6",
    "jest-junit": "^12",
    "json-schema": "^0.3.0",
    "npm-check-updates": "^11",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.110.0",
    "@aws-cdk/aws-cloudfront": "1.110.0",
    "@aws-cdk/aws-cloudfront-origins": "1.110.0",
    "@aws-cdk/aws-dynamodb": "1.110.0",
    "@aws-cdk/aws-ec2": "1.110.0",
    "@aws-cdk/aws-ecs": "1.110.0",
    "@aws-cdk/aws-ecs-patterns": "1.110.0",
    "@aws-cdk/aws-iam": "1.110.0",
    "@aws-cdk/aws-lambda": "1.110.0",
    "@aws-cdk/aws-logs": "1.110.0",
    "@aws-cdk/aws-s3": "1.110.0",
    "@aws-cdk/core": "1.110.0",
    "@aws-solutions-constructs/aws-apigateway-lambda": "1.110.0",
    "@aws-solutions-constructs/aws-cloudfront-apigateway-lambda": "1.110.0",
    "@aws-solutions-constructs/aws-cloudfront-s3": "1.110.0",
    "@aws-solutions-constructs/core": "1.110.0"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cdk.out/"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/",
      "/cdk.out/"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "clearMocks": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "clover",
      "text"
    ],
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/cdk.out/"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test-reports"
        }
      ]
    ],
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.jest.json"
      }
    }
  }
}
